// move all negative element to one side of an array
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++) cin>>arr[i];
    int pivot=0;
    int i=-1;
    for(int j=0;j<n;j++)
    {
        if(arr[j]<pivot)
        {
            i++;
            swap(arr[j],arr[i]);
        }
    }
    for(int i=0;i<n;i++) cout<<arr[i]<<" ";
    return 0;
}
